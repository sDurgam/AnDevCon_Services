<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.callmeike.android.services.app0"
    >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".CookieActivity"/>
        <activity android:name=".PrefixActivity"/>

        <service android:name=".CookieService" />

        <service
            android:name=".LocalService1"
            android:enabled="true"
            android:exported="false"
            />

        <service
            android:name=".LocalService2"
            android:enabled="true"
            android:exported="false"
            />
    </application>

</manifest>
